{
 "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
 "title": "Life Expectancy of people in countries around the world ",
 "width": 800,
 "height": 600,
 "projection": {"type": "equirectangular"},
 "data": {
 "url": "https://raw.githubusercontent.com/David1284/FIT3179/main/map.topojson",
 "format": {"type": "topojson", "feature": "map"}
 },
 "transform": [
 {
 "lookup": "properties.NAME",
 "from": {
 "data": {
 "url": "https://raw.githubusercontent.com/David1284/FIT3179/main/csvData.csv"
 },
 "key": ""name"",
 "fields": [""total""]
  }
 },
 {"calculate": "datum."total" + 0.1", "as": "Life Expectancy"}
 ],
 "mark": {"type": "geoshape"},
 "encoding": {
 "color": {
 "field": "Life Expectancy",
 "type": "quantitative",
 "scale": {"type": "log"}
 },
 "tooltip": [
 {"field": "properties.NAME", "type": "nominal", "title": ""name""},
 {"field": ""total"", "type": "quantitative"}
 ]
 }
}
